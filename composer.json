{
	"name": "dougw/phplangtest",
	"description": "PHP Language Tests",
	"type": "project",
	"authors": [
		{
			"name": "Doug Wilbourne",
			"email": "dougwilbourne@gmail.com"
		}
	],
	"license": "MIT",
	"config": {
		"php": "8.1"
	},
	"autoload": {
		"psr-4": {
			"tests\\": "tests"
		}
	},
	"require": {
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"ext-iconv": "*",
		"ext-gettext": "*",
		"ext-dom": "*",
		"ext-libxml": "*",
		"ext-simplexml": "*",
		"ext-tidy": "*",
		"ext-mbstring": "*",
		"ext-exif": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"squizlabs/php_codesniffer": "*"
	}

}
